{
  "version": 2,
  "builds": [
    {
      "src": "dist/portafolio/server/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/portafolio/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/portafolio/server/main.js"
    }
  ]
}
